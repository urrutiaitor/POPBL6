	private String[] getTempHistorial() {
		JDBC jdbc = new JDBC();
		
		ArrayList<Temperatura> temperaturas = jdbc.getTemperatura();
		String[] list = new String[temperaturas.size()];
		
		for (int i = 0; i < temperaturas.size(); i++) {
			list[i] = temperaturas.get(i).toString();
			
		}
		
		return list;
	}
}
